<p id="notice"><%= notice %></p>

<h1>Listing Aggressors</h1>

<table>
  <thead>
    <tr>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @aggressors.each do |aggressor| %>
      <tr>
        <td><%= aggressor.type %></td>
        <td><%= link_to 'Show', aggressor %></td>
        <td><%= link_to 'Edit', edit_aggressor_path(aggressor) %></td>
        <td><%= link_to 'Destroy', aggressor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Aggressor', new_aggressor_path %>
